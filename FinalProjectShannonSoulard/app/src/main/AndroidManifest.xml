<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finalprojectshannonsoulard">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="StockView"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProjectShannonSoulard">
        <activity android:name=".Home"></activity>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity
            android:name=".SearchDetails"
            android:parentActivityName=".Search">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Search" />
        </activity>
        <activity android:name=".Bookmarks" />
        <activity android:name=".Search" />
        <activity android:name=".Profile" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>